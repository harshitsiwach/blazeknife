{
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://twitter.com https://x.com https://cards-dev.twitter.com;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://twitter.com"
        }
      ]
    }
  ]
}
